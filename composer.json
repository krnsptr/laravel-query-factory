{
  "name": "krnsptr/laravel-query-factory",
  "description": "Query Factory use for easily mock your query builder without sending query from real database",
  "keywords": [
    "laravel",
    "factory",
    "unit",
    "test",
    "query",
    "lumen",
    "mock",
    "database"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Cydrick Nonog",
      "email": "cydrick.dev@gmail.com"
    },
    {
      "name": "Kurnia Saputra",
      "email": "krnsptr@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "laravel/framework": "~5.5|~6.0|~7.0|~8.0",
    "ext-pdo": "*",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0|~8.0|~9.0",
    "laravel/laravel": "~5.5|~6.0|~7.0",
    "mockery/mockery": "^1.0",
    "ext-pdo_mysql": "*",
    "php-coveralls/php-coveralls": "^2.2"
  },
  "autoload": {
    "psr-4": {
      "LaravelQueryFactory\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LaravelQueryFactory\\Tests\\": "tests/"
    }
  }
}
